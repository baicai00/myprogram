user                kk;
worker_processes    1;

error_log logs/error_log debug;

pid     logs/nginx.pid;

events {
    worker_connections  1024;
}

stream {
    lua_package_path "${prefix}/lua/?.lua;;";

    server {
        listen 192.168.161.138:8881;
        content_by_lua_file lua/test_websocket.lua;
    }
}
